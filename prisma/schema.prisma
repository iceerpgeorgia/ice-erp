   Counteragents (exact spec)
   Column order matches your list.
   No relations/constraints yet.
   ÃƒÂ¢Ã¢â‚¬ÂÃ¢â€šÂ¬ÃƒÂ¢Ã¢â‚¬ÂÃ¢â€šÂ¬ÃƒÂ¢Ã¢â‚¬ÂÃ¢â€šÂ¬ÃƒÂ¢Ã¢â‚¬ÂÃ¢â€šÂ¬ÃƒÂ¢Ã¢â‚¬ÂÃ¢â€šÂ¬ÃƒÂ¢Ã¢â‚¬ÂÃ¢â€šÂ¬ÃƒÂ¢Ã¢â‚¬ÂÃ¢â€šÂ¬ÃƒÂ¢Ã¢â‚¬ÂÃ¢â€šÂ¬ÃƒÂ¢Ã¢â‚¬ÂÃ¢â€šÂ¬ÃƒÂ¢Ã¢â‚¬ÂÃ¢â€šÂ¬ÃƒÂ¢Ã¢â‚¬ÂÃ¢â€šÂ¬ÃƒÂ¢Ã¢â‚¬ÂÃ¢â€šÂ¬ÃƒÂ¢Ã¢â‚¬ÂÃ¢â€šÂ¬ÃƒÂ¢Ã¢â‚¬ÂÃ¢â€šÂ¬ÃƒÂ¢Ã¢â‚¬ÂÃ¢â€šÂ¬ÃƒÂ¢Ã¢â‚¬ÂÃ¢â€šÂ¬ÃƒÂ¢Ã¢â‚¬ÂÃ¢â€šÂ¬ÃƒÂ¢Ã¢â‚¬ÂÃ¢â€šÂ¬ÃƒÂ¢Ã¢â‚¬ÂÃ¢â€šÂ¬ÃƒÂ¢Ã¢â‚¬ÂÃ¢â€šÂ¬ÃƒÂ¢Ã¢â‚¬ÂÃ¢â€šÂ¬ÃƒÂ¢Ã¢â‚¬ÂÃ¢â€šÂ¬ÃƒÂ¢Ã¢â‚¬ÂÃ¢â€šÂ¬ÃƒÂ¢Ã¢â‚¬ÂÃ¢â€šÂ¬ÃƒÂ¢Ã¢â‚¬ÂÃ¢â€šÂ¬ÃƒÂ¢Ã¢â‚¬ÂÃ¢â€šÂ¬ÃƒÂ¢Ã¢â‚¬ÂÃ¢â€šÂ¬ÃƒÂ¢Ã¢â‚¬ÂÃ¢â€šÂ¬ */
model Counteragent {
  id                          BigInt    @id @default(autoincrement())
  createdAt                   DateTime  @default(now())   @map("created_at")
  updatedAt                   DateTime  @updatedAt        @map("updated_at")
  ts                          DateTime  @default(now())   @db.Timestamptz(6)

  name                        String?   @db.Text
  identification_number       String?   @db.Text
  birth_or_incorporation_date DateTime?

  entity_type                 String?   @db.Text
  sex                         String?   @db.Text
  pension_scheme              Boolean?

  country                     String?   @db.Text

  address_line_1              String?   @db.Text
  address_line_2              String?   @db.Text
  zip_code                    String?   @db.Text
  iban                        String?   @db.Text
  swift                       String?   @db.Text
  director                    String?   @db.Text
  director_id                 String?   @db.Text
  email                       String?   @db.Text
  phone                       String?   @db.Text
  oris_id                     String?   @db.Text

  counteragent                String?   @db.Text

  country_uuid                String?   @db.Uuid
  entity_type_uuid            String?   @db.Uuid
  counteragent_uuid           String     @db.Uuid @default(uuid())
  internal_number             String?   @db.Text

  is_emploee Boolean @default(false)

  auditLogs CounteragentAuditLog[]

  @@map("counteragents")
}

/* ═══════════════════════════════════════════════════════════════
   Counteragent Audit Log
   ═══════════════════════════════════════════════════════════════ */
model CounteragentAuditLog {
  id               BigInt   @id @default(autoincrement())
  counteragent_id  BigInt
  changed_at       DateTime @default(now()) @db.Timestamptz(6)
  changed_by       String?  @db.Text
  field_name       String   @db.Text
  old_value        String?  @db.Text
  new_value        String?  @db.Text
  operation        String   @db.Text

  counteragent Counteragent @relation(fields: [counteragent_id], references: [id], onDelete: Cascade)

  @@map("counteragent_audit_log")
  @@index([counteragent_id])
  @@index([changed_at])
}
